    "lint": "next lint",
    "export": "next build",
    "deploy": "npm run build && touch out/.nojekyll"
  },
  "dependencies": {
    "date-fns": "^3.0.6",
    "firebase": "^10.14.1",
    "firebase-admin": "^12.0.0",
    "next": "^14.0.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@tailwindcss/line-clamp": "^0.4.4",
    "@types/node": "^20.10.6",
    "@types/react": "^18.2.46",
    "@types/react-dom": "^18.2.18",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.56.0",
    "eslint-config-next": "^14.0.4",
    "postcss": "^8.4.32",
    "sass": "^1.69.5",
    "tailwindcss": "^3.4.0",
    "typescript": "^5.3.3"
  }
}
